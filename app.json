{
  "expo": {
    "scheme": "stellar",
    "name": "stellar-app",
    "slug": "stellar-app",
    "version": "1.0.0",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#181818"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.codewilson64.stellarapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "51596189-60b8-4065-8057-f9ef940a7c67"
      }
    },
    "owner": "codewilson64"
  }
}
